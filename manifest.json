{
  "name": "neptun extension project",
  "description": "egy bővülő neptun bővítmény",
  "version": "0.1.2",
  "homepage_url": "https://neptun-extension-project.github.io/",
  "manifest_version": 3,
  "permissions": [
    "declarativeNetRequest",
    "storage",
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_title": "NEP",
    "default_popup": "options.html",
    "default_icon": {
      "16": "icon.svg",
      "24": "icon.svg",
      "48": "icon.svg",
      "96": "icon.svg"
    }
  },
  "icons": {
    "16": "icon.svg",
    "24": "icon.svg",
    "48": "icon.svg",
    "96": "icon.svg"
  },
  "options_ui": {
    "page": "options.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "nep@neptun-extension-project.github.io"
    }
  },
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_bypass_2fa",
      "enabled": true,
      "path": "modules/bypass_2fa/rules.json"
    }]
  },
  "content_scripts": [{
    "js": ["contentscript.js"],
    "matches": [
      "https://neptun.bme.hu/*",
      "https://neptun.ejf.hu/*",
      "https://neptun.elte.hu/*",
      "https://*.neptun.u-szeged.hu/*",
      "https://*.uni-bge.hu/*",
      "https://neptun.uni-obuda.hu/*",
      "https://*.metropolitan.hu/*",
      "https://*.tr.pte.hu/*",
      "https://neptun-web2.tr.pte.hu/hallgatoing/*",
      "https://neptunhallgato.uni-mate.hu/*",
      "https://neptun3r.web.uni-corvinus.hu/*",
      "https://neptun.tf.hu/*",
      "https://*.nnet.sze.hu/*",
      "https://neptunweb.uni-nke.hu/*",
      "https://*.uni-miskolc.hu/*",
      "https://neptunweb.semmelweis.hu/*",
      "https://neptun.kre.hu/*",
      "https://nappw.dfad.duf.hu/*",
      "https://*.uni-pannon.hu/*",
      "https://*.kefo.hu/*",
      "https://*.ppke.hu/*",
      "https://neptun.lfze.hu/*",
      "https://neptun.edutus.hu/*",
      "https://neptun3r.nyme.hu/*",
      "https://neptun.uni-milton.hu/*",
      "https://host.sdakft.hu/*",
      "https://neptun.uni-eszterhazy.hu/*",
